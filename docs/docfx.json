{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "vsdocsdocs/**",
          "**/includes/**"
        ],
        "src": "./",
        "group": "vs"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "vsdocsdocs/**",
          "**/includes/**"
        ],
        "src": "./",
        "group": "vs"
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "feedback_system": "None",
      "feedback_github_repo": "",
      "feedback_product_url": "",
      "breadcrumb_path": "/visualstudio/_breadcrumb/toc.json",
      "bilingual_type": "hover over",
      "extendBreadcrumb": "true",
      "manager": "jillfra",
      "ms.topic": "conceptual",
      "audience": "developer",
      "defaultDevLang": "csharp",
      "ms.prod": "visual-studio-windows",
      "uhfHeaderId": "MSDocsHeader-VisualStudio",
      "titleSuffix": "Visual Studio (Windows)",
      "toc_preview": true,
      "searchScope": [
        "VS IDE"
      ],
      "featureFlags": [
        "enable_uhf_ppe"
      ],
      "archive_url": "/previous-versions/visualstudio",
      "disable_contribution_on_translation_quality": [
        "MT",
        "MTE75",
        "MTE95",
        "Blank"
      ]
    },
    "fileMetadata": {
      "ms.prod": {
        "rtvs/**/**.md": "visual-studio-dev15"
      },
      "monikerRange": {
        "rtvs/**/**.md": "vs-2017"
      },
      "ms.technology": {
        "ai/**/**.md": "vs-ai-tools",
        "azure/**/**.md": "vs-azure",
        "code-quality/**/**.md": "vs-ide-code-analysis",
        "data-tools/**/**.md": "vs-data-tools",
        "debugger/**/**.md": "vs-ide-debug",
        "deployment/**/**.md": "vs-ide-deployment",
        "designers/**/**.md": "vs-ide-designers",
        "extensibility/**/**.md": "vs-ide-sdk",
        "help-viewer/**/**.md": "vs-help-viewer",
        "ide/**/**.md": "vs-ide-general",
        "install/**/**.md": "vs-installation",
        "javascript/**/**.md": "vs-javascript",
        "modeling/**/**.md": "vs-ide-modeling",
        "msbuild/**/**.md": "msbuild",
        "profiling/**/**.md": "vs-ide-debug",
        "python/**/**.md": "vs-python",
        "rtvs/**/**.md": "vs-rtvs",
        "sharepoint/**/**.md": "sharepoint-development",
        "test/**/**.md": "vs-ide-test",
        "vsto/**/**.md": "office-development",
        "workflow-designer/**/**.md": "vs-workflow-designer",
        "xaml-tools/**/**.md": "vs-xaml-tools",
        "xml-tools/**/**.md": "vs-xml-tools"
      },
      "titleSuffix": {
        "ai/**/**.md": "AI tools for Visual Studio"
      },
      "feedback_system": {
        "vs-2015/**/*": "None",
        "extensibility/**/*": "None"
      },
      "searchScope": {
        "extensibility/**/*.md": [
          "VS Extensibility"
        ]
      }
    },
    "template": [],
    "dest": "vsdocsdocs",
    "markdownEngineName": "markdig",
    "xref": [],
    "groups": {
      "vs": {
        "dest": "vs-dest",
        "moniker_range": ">= vs-2017"
      }
    }
  }
}